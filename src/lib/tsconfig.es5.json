{
  "extends": "./tsconfig.lib.json",
  "compilerOptions": {
    "target": "es5",
    "outDir": "../../out-tsc/lib-es5/",
    "baseUrl": "",
    "typeRoots": [
      "node_modules/@types"
    ]
  },
  "files": [
    "./index.ts",
    "./typings.d.ts"
